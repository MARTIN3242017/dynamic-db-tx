<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.samh.mapper.OrderMapper">
    <update id="addAge">
        update user
        set age = age + 1
        where id = 1
    </update>
</mapper>
